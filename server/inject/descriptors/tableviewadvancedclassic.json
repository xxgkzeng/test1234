{
  "type": "object",
  "comment": "issueslist",
  "properties": {
    "issueslist": {
      "type": "object",
      "update": {
        "type": "none"
      },
      "frameSelector": "iframe[name='pageIframe']",
      "selector": "#REQHeadTable tbody tr",
      "properties": {
        "RFQNO": {
          "type": "string",
          "update": {
            "type": "none"
          },
          "selector": "td",
          "control": "property.innerText"
        },
        "ApplicateDate": {
          "type": "string",
          "update": {
            "type": "none"
          },
          "selector": "td",
          "control": "property.innerHTML"
        },
        "PrepareBy": {
          "type": "string",
          "update": {
            "type": "none"
          },
          "selector": "td",
          "control": "property.innerHTML"
        }
      }
    },
    "isview": {
      "type": "identifier",
      "control": "exists",
      "selectContains": "RFQNO#",
      "frameSelector": "iframe[name='pageIframe']",
      "selector": ".th-inner"
    }
  },
  "connect": [
    {
      "match": [
        "isview"
      ],
      "screens": [
        "tableviewadvancedclassic"
      ]
    }
  ]
}

          },
          "ApplicateDate": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "td",
            "control": "property.innerHTML"
          },
          "PrepareBy": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "td",
            "control": "property.innerHTML"
          }
        }
      },
      "update": {
        "type": "none"
      },
      "selectorAll": "#REQHeadTable tbody tr",
      "frameSelector": "iframe[name='pageIframe']"
    },
    "isview": {
      "type": "identifier",
      "control": "exists",
      "selector": ".th-inner.",
      "selectContains": "RFQNO#"
    }
  },
  "connect": [
    {
      "match": [
